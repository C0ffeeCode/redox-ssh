    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ssh-keygen'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ssh-keygen",
                    "--package=redox-ssh"
                ],
                "filter": {
                    "name": "ssh-keygen",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'sshd'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=sshd",
                    "--package=redox-ssh"
                ],
                "filter": {
                    "name": "sshd",
                    "kind": "bin"
                }
            },
            "env": {
                "RUST_BACKTRACE": "1"
            },
            "args": [
                "-p", "2222",
                "-vvv",
                "-f"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}